{
    "style": "klokantech-basic",
    "latitude": #(lat),
    "longitude": #(lon),
    "zoom": #(zoom),
    "width": 400,
    "height": 400,
    "scale": 2,
    "markers": [
    	{
    	   "url": "#(url)",
    	   "latitude": #(lat),
    	   "longitude": #(lon),
    	   "width": 400,
    	   "height": 400
    	},
      {
         "url": "#(url2)",
         "latitude": #(lat),
         "longitude": #(lon),
         "width": 400,
         "height": 400
      }
    ]
}
